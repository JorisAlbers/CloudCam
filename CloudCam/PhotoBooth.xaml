<reactiveUi:ReactiveUserControl x:TypeArguments="local:PhotoBoothViewModel" x:Class="CloudCam.PhotoBooth"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                                xmlns:local="clr-namespace:CloudCam"
                                xmlns:reactiveUi="http://reactiveui.net"
                                mc:Ignorable="d" 
                                d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image x:Name="VideoImage" Grid.Row="0" Stretch="Uniform"/>
        <Image x:Name="FrameImage" Grid.Row="0" Stretch="Uniform"/>
    </Grid>
</reactiveUi:ReactiveUserControl>
